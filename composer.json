{
    "name": "yii-tools/vanillajs-datepicker-asset",
    "type": "library",
    "description": "Asset for VanillaJs DatePicker for YiiFramework v.3.0.",
    "keywords": [
        "yii-tools",
        "asset",
        "vanillajs-datepicker",
        "yii3"
    ],
    "license": "mit",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "foxy/foxy": "^1.2",
        "yiisoft/assets": "^3.0",
        "yiisoft/files": "^2.0"
    },
    "require-dev": {
        "maglnet/composer-require-checker": "^4.3",
        "phpunit/phpunit": "^10.0",
        "roave/infection-static-analysis-plugin": "^1.28",
        "vimeo/psalm": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Yii\\DatePicker\\Asset\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yii\\DatePicker\\Asset\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true,
            "foxy/foxy": true
        }
    },
    "scripts": {
        "check-dependencies": "composer-require-checker",
        "mutation": "roave-infection-static-analysis-plugin",
        "psalm": "psalm",
        "test": "phpunit"
    }
}
